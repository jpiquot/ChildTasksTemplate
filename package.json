{
    "name": "child-tasks-template",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "main": "src/app.js",
    "scripts": {
        "clean": "rimraf ./dist ./bin",
        "compile": "npm run clean && webpack --mode production",
        "compile:dev": "npm run clean && webpack --mode development",
        "package": "npm run compile && tfx extension create --output-path ./bin --manifest-globs vss-extension.json",
        "package:dev": "npm run compile:dev && tfx extension create --output-path ./bin --manifest-globs vss-extension.json --overrides-file vss-extension-dev.json",
        "publish": "npm run compile && tfx extension publish --output-path ./bin --manifest-globs vss-extension.json",
        "publish:dev": "npm run compile:dev && tfx extension publish --output-path ./bin --manifest-globs vss-extension.json --overrides-file vss-extension-dev.json",
        "start:dev": "webpack serve --mode development --env development --port 6221"
    },
    "dependencies": {
        "react": "^16.8.1",
        "react-dom": "^16.8.1"
    },
    "devDependencies": {
        "@types/jest": "~26.0.23",
        "@types/jquery": "^3.5.5",
        "@types/jsoneditor": "^8.6.0",
        "@types/node": "^14.14.45",
        "@types/react": "^16.8.1",
        "@types/react-dom": "^16.8.1",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "azure-devops-extension-api": "^1.158.0",
        "azure-devops-extension-sdk": "^2.0.11",
        "azure-devops-ui": "^2.167.2",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "copy-webpack-plugin": "^8.1.1",
        "css-loader": "^5.2.4",
        "eslint": "^7.26.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.1",
        "jsoneditor": "^9.4.1",
        "node-sass": "^5.0.0",
        "pupa": "^2.1.1",
        "resolve-url-loader": "^3.1.3",
        "rimraf": "^3.0.2",
        "sass-loader": "^11.1.1",
        "style-loader": "^2.0.0",
        "tfx-cli": "^0.9.2",
        "ts-loader": "^8.2.0",
        "typescript": "~4.2.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.37.0",
        "webpack-cli": "^4.7.0",
        "webpack-dev-server": "^3.11.2"
    }
}
